<UserControl x:Class="Insurgency_theater_editor.Content_preset.CollectionPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Insurgency_theater_editor.Content_preset"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Border Margin="5" BorderBrush="Black" BorderThickness="1">
        <Expander>
            <Expander.Header>
                <ComboBox x:Name="Header" SelectionChanged="ComboBox_SelectionChanged" IsEditable="True">
                </ComboBox>
            </Expander.Header>
            <StackPanel>
                <StackPanel x:Name="Data"></StackPanel>
                <StackPanel x:Name="ToolBar" Orientation="Horizontal">
                    <Button x:Name="ContentCreateButtion" Margin="2" Click="Button_AddContent">Add new value</Button>
                    <Button x:Name="CollectionCreateButtion" Margin="2" Click="Button_AddCollection">Add new collection</Button>
                </StackPanel>
            </StackPanel>
        </Expander>
    </Border>
</UserControl>
